{
  "name": "app-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vue/test-utils": "^1.1.3",
    "core-js": "^3.6.5",
    "jest-serializer-vue": "^2.0.2",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.7"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
